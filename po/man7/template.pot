#: man-pages/man7/ip.7:512
#, no-wrap
msgid "IP_PMTUDISC_DONT"
msgstr ""

#. type: tbl table
#: man-pages/man7/ip.7:512
#, no-wrap
msgid "Never do Path MTU Discovery."
msgstr ""

#. type: tbl table
#: man-pages/man7/ip.7:513
#, no-wrap
msgid "IP_PMTUDISC_DO"
msgstr ""

#. type: tbl table
#: man-pages/man7/ip.7:513
#, no-wrap
msgid "Always do Path MTU Discovery."
msgstr ""

