#: man-pages/man7/ip.7:511
#, no-wrap
msgid "IP_PMTUDISC_WANT"
msgstr ""

#. type: tbl table
#: man-pages/man7/ip.7:511
#, no-wrap
msgid "Use per-route settings."
msgstr ""

