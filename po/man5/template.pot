msgstr ""

#. type: TP
#: man-pages/man5/sysfs.5:230
#, no-wrap
msgid "I<refcnt>"
msgstr ""

#. type: TP
#: man-pages/man5/sysfs.5:233
#, no-wrap
msgid "I<srcversion>"
msgstr ""

#. type: TP
#: man-pages/man5/sysfs.5:236
#, no-wrap
msgid "I<taint>"
msgstr ""

#. type: TP
#: man-pages/man5/sysfs.5:239
#, no-wrap
msgid "I<uevent>"
msgstr ""

#. type: TP
#: man-pages/man5/sysfs.5:242
