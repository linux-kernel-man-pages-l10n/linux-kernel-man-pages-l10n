msgid "ram - ram disk device"
msgstr ""

#. type: Plain text
#: man-pages/man4/ram.4:33
msgid "The I<ram> device is a block device to access the ram disk in raw mode."
msgstr ""

#. type: Plain text
#: man-pages/man4/ram.4:40
#, no-wrap
msgid ""
"mknod -m 660 /dev/ram b 1 1\n"
"chown root:disk /dev/ram\n"
msgstr ""

#. type: Plain text
