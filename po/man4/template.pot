msgstr ""

#. type: Plain text
#: man-pages/man4/full.4:61
msgid "Seeks on I</dev/full> will always succeed."
msgstr ""

#. type: Plain text
#: man-pages/man4/full.4:63
msgid "I</dev/full>"
msgstr ""

#. type: Plain text
