#, no-wrap
msgid ""
"TZ=\"Pacific/Honolulu\"\n"
"-\t-\t-10:31:26\tLMT\n"
"1896-01-13\t12:01:26\t-10:30\tHST\n"
"1933-04-30\t03\t-09:30\tHDT\t1\n"
"1933-05-21\t11\t-10:30\tHST\n"
