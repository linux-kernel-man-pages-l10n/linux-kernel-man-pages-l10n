#: man-pages/man1/ldd.1:112 man-pages/man1/mtrace.1:55
#, no-wrap
msgid "B<--version>"
msgstr "B<--version>"

#. type: Plain text
#: man-pages/man1/ldd.1:116
msgid "Print the version number of B<ldd>."
msgstr "Выдать номер версии B<ldd>."

#. type: TP
#: man-pages/man1/ldd.1:116 man-pages/man1/locale.1:117
#: man-pages/man1/localedef.1:218
#, no-wrap
msgid "B<-v>, B<--verbose>"
msgstr "B<-v>, B<--verbose>"

#. type: Plain text
#: man-pages/man1/ldd.1:120
msgid ""
"Print all information, including, for example, symbol versioning information."
msgstr ""
"Выдать всю информацию, включая, например, информацию о версии символов."

#. type: TP
#: man-pages/man1/ldd.1:120
#, no-wrap
msgid "B<-u>, B<--unused>"
msgstr "B<-u>, B<--unused>"

