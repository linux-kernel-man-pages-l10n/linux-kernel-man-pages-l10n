msgstr ""

#. type: TP
#: man-pages/man7/man.7:205
#, no-wrap
msgid "B<\\&.P>"
msgstr ""

#. type: TP
#: man-pages/man7/man.7:210
#, no-wrap
msgid "B<\\&.PP>"
msgstr ""

