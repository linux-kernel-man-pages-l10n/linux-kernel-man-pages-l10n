"        return -1;\n"
"    }\n"
msgstr ""

#. type: Plain text
#: man-pages/man2/select_tut.2:572
#, no-wrap
msgid ""
"    yes = 1;\n"
"    if (setsockopt(lfd, SOL_SOCKET, SO_REUSEADDR,\n"
