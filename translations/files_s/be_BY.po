msgstr ""

#. type: Plain text
#: man-pages/man2/select_tut.2:581
#, no-wrap
msgid ""
"    memset(&addr, 0, sizeof(addr));\n"
"    addr.sin_port = htons(listen_port);\n"
"    addr.sin_family = AF_INET;\n"
