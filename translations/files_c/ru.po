msgstr "cfree - освобождает выделенную память"

#. type: Plain text
#: man-pages/man3/cfree.3:34
#, no-wrap
msgid ""
"/* In SunOS 4 */\n"
"B<int cfree(void *>I<ptr>B<);>\n"
msgstr ""
"/* в SunOS 4 */\n"
"B<int cfree(void *>I<ptr>B<);>\n"

#. type: Plain text
#: man-pages/man3/cfree.3:37
#, no-wrap
msgid ""
