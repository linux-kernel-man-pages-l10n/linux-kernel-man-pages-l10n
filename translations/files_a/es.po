msgstr ""

#. type: Plain text
#: man-pages/man3/atan2.3:38
msgid "atan2, atan2f, atan2l - arc tangent function of two variables"
msgstr ""

#. type: Plain text
#: man-pages/man3/atan2.3:41
#, no-wrap
msgid "B<#include E<lt>math.hE<gt>>\n"
msgstr ""

