".UE>."

#. type: TP
#: man-pages/man7/address_families.7:327
#, no-wrap
msgid "B<AF_PHONET>"
msgstr "B<AF_PHONET>"

#.  commit: 4b07b3f69a8471cdc142c51461a331226fef248a
#. type: Plain text
#: man-pages/man7/address_families.7:334
