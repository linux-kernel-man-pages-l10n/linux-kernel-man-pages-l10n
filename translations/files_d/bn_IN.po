#. type: TP
#: man-pages/man3/dlinfo.3:100
#, no-wrap
msgid "B<RTLD_DI_ORIGIN> (I<char *>)"
msgstr ""

#. type: Plain text
#: man-pages/man3/dlinfo.3:106
msgid ""
