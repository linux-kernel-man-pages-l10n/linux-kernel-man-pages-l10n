"    for (i = if_ni; ! (i-E<gt>if_index == 0 && i-E<gt>if_name == NULL); i++)\n"
"        printf(\"%u: %s\\en\", i-E<gt>if_index, i-E<gt>if_name);\n"
msgstr ""
