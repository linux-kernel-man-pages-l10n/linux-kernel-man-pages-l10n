msgstr ""

#. type: Plain text
#: man-pages/man1/ldd.1:120
msgid ""
"Print all information, including, for example, symbol versioning "
"information."
msgstr ""

#. type: TP
#: man-pages/man1/ldd.1:120
#, no-wrap
msgid "B<-u>, B<--unused>"
msgstr ""

#. type: Plain text
#: man-pages/man1/ldd.1:124
msgid "Print unused direct dependencies.  (Since glibc 2.3.4.)"
msgstr ""

#. type: TP
#: man-pages/man1/ldd.1:124
#, no-wrap
msgid "B<-d>, B<--data-relocs>"
msgstr ""

#. type: Plain text
#: man-pages/man1/ldd.1:127
