"$ B<echo \"export LOCPATH=\\e$HOME/.locale\" E<gt>E<gt> $HOME/.bashrc>\n"
"$ B<echo \"export LANG=fi_SE.UTF-8\" E<gt>E<gt> $HOME/.bashrc>\n"
