#. type: Plain text
#: man-pages/man3/nl_langinfo.3:138
#, no-wrap
msgid ""
"int\n"
"main(int argc, char *argv[])\n"
"{\n"
"    setlocale(LC_CTYPE, \"\");\n"
"    setlocale(LC_NUMERIC, \"\");\n"
"    printf(\"%s\\en\", nl_langinfo(CODESET));\n"
"    printf(\"%s\\en\", nl_langinfo(RADIXCHAR));\n"
"    exit(EXIT_SUCCESS);\n"
"}\n"
msgstr ""
"int\nmain(int argc, char *argv[])\n{\n    setlocale(LC_CTYPE, \"\");\n    "
"setlocale(LC_NUMERIC, \"\");\n    printf(\"%s\\en\", nl_langinfo(CODESET));\n "
"   printf(\"%s\\en\", nl_langinfo(RADIXCHAR));\n    exit(EXIT_SUCCESS);\n}\n"

